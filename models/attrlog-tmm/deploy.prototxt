layer {
  name: "relu7"
  type: "ReLU"
  bottom: "fc7"
  top: "fc7"
}
layer {
  name: "drop7"
  type: "Dropout"
  bottom: "fc7"
  top: "fc7"
  dropout_param {
    dropout_ratio: 0.5
  }
}

layer {
  name: "score22"
  type: "Convolution"
  bottom: "fc7"
  top: "score22"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 23
    kernel_size: 1
    engine: CAFFE
  }
}

#==============================================
# Attr fork.
#==============================================
layer {
  name: "attr_fc8"
  type: "Convolution"
  bottom: "fc7"
  top: "attr_fc8"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  convolution_param {
    num_output: 256
    kernel_h: 19
    kernel_w: 13
    engine: CAFFE
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 1 }
  }
}
layer {
  name: "attr_relu8"
  type: "ReLU"
  bottom: "attr_fc8"
  top: "attr_fc8"
}
layer {
  name: "attr_drop8"
  type: "Dropout"
  bottom: "attr_fc8"
  top: "attr_fc8"
  dropout_param {
    dropout_ratio: 0.5
  }
}

layer {
  name: "attr_score22"
  type: "Convolution"
  bottom: "attr_fc8"
  top: "attr_score22"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  convolution_param {
    num_output: 23
    kernel_size: 1
    engine: CAFFE
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 1 }
  }
}

#==========================================================
# add sigmoid to filter labels
#==========================================================
layer {
  name: "attr_sigmoid"
  bottom: "attr_score22"
  top: "attr_score22_sigmoid"
  type: "Sigmoid"
}

layer {
  name: "attr_broadcast_correct"
  type: "Deconvolution"
  bottom: "attr_score22_sigmoid"
  top: "attr_bigscore22"
  param { lr_mult: 0 }
  convolution_param {
    num_output: 23
    bias_term: false
    kernel_h: 82
    kernel_w: 58
    weight_filler { type: "constant" value: 1 }
    group: 23
  }
}


#==============================================
# End: Attr fork.
#==============================================

layer {
  name: "upscoretmm16s"
  type: "Deconvolution"
  bottom: "score22"
  top: "upscoretmm16s"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  convolution_param {
    num_output: 23
    bias_term: false
    kernel_size: 4
    stride: 2
  }
}

layer {
  name: "score-pool4"
  type: "Convolution"
  bottom: "pool4"
  top: "score-pool4"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 23
    kernel_size: 1
    engine: CAFFE
  }
}

layer { type: "Crop" name: "crop1"
  bottom: "score-pool4" bottom: "upscoretmm16s"
  top: "score-pool4c" }

layer {
  name: "fuse1"
  type: "Eltwise"
  bottom: "upscoretmm16s"
  bottom: "score-pool4c"
  top: "score-fused"
  eltwise_param {
    operation: SUM
  }
}

layer {
  name: "upsample_fused_16"
  type: "Deconvolution"
  bottom: "score-fused"
  top: "score4"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  convolution_param {
    num_output: 23
    bias_term: false
    kernel_size: 4
    stride: 2
  }
}
layer {
  name: "score-pool3"
  type: "Convolution"
  bottom: "pool3"
  top: "score-pool3"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 23
    kernel_size: 1
    engine: CAFFE
  }
}
layer { 
  name: "crop2"
  type: "Crop" 
  bottom: "score-pool3" 
  bottom: "score4"
  top: "score-pool3c" 
}

layer {
  name: "fuse2"
  type: "Eltwise"
  bottom: "score4"
  bottom: "score-pool3c"
#  bottom: "attr_bigscore24"
  top: "score-final"
  eltwise_param {
    operation: SUM
  }
}

#==========================================================
# filter by eltwise product
#==========================================================
layer{
 name: "fuse_attr_filt"
 type: "Eltwise"
 bottom: "score-final"
 bottom: "attr_bigscore22"
 top: "seg_score_filt"
 eltwise_param {
   operation: PROD
 }
}

layer {
  name: "upsample"
  type: "Deconvolution"
  bottom: "seg_score_filt"
  top: "bigscore"
  param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 23
    bias_term: false
    kernel_size: 16
    stride: 8
  }
}


#change name from 'crop' to 'seg_score'
layer { 
  #name: 'seg_score'
  name: 'crop'
  type: 'Crop'  
  bottom: 'bigscore' 
  bottom: 'image' 
  top: 'score' 
}

layer {
  name: "softmax"
  type: "Softmax"
  bottom: "score"
  top: "seg_prob"
}
